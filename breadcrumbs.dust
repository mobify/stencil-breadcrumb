<nav class="c-breadcrumbs {modifier}" role="navigation" data-adaptivejs-component="stencil-breadcrumbs">
    <ol class="c-breadcrumbs__list">
        {?element}
            {element}
        {:else}
            {#items}
                <li class="c-breadcrumbs__item">
                    {?href}<a href="{href}" class="c-breadcrumbs__link">{/href}{label}{?href}</a>{/href}
                </li>
            {/items}
        {/element}
    </ol>
</nav>
