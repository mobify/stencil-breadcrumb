<ol class="c-breadcrumbs" role="navigation" data-adaptivejs-component="stencil-breadcrumbs">
    {?element}
        {element|s}
    {:else}
        {#items}
            <li class="c-breadcrumbs__item {class}">
                {?href}<a href="{href}" class="c-breadcrumbs__link">{/href}{label}{?href}</a>{/href}
            </li>
        {/items}
    {/element}
</ol>
